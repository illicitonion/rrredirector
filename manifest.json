{
  "manifest_version": 2,

  "name": "rrredirector",
  "description": "Redirects github.com pull request pages to review.rocks",
  "version": "1.2",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "browser_action.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://github.com/"
  ]
}
